{
    "name": "t7team/smscru",
    "description": "SmscRu Notifications channel for Laravel 8.",
    "keywords": [
        "laravel",
        "notifications",
        "smscentre",
        "smsc",
        "СМС-Центр"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "JhaoDa",
            "role": "Developer",
            "email": "jhaoda@gmail.com",
            "homepage": "https://github.com/jhaoda"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/queue": "5.1 - 5.8|^6.0|^7.0|^8.0",
        "illuminate/notifications": "5.1 - 5.8|^6.0|^7.0|^8.0",
        "illuminate/support": "5.1 - 5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "T7team\\SmscRu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T7team\\SmscRu\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "T7team\\SmscRu\\SmscRuServiceProvider"
            ]
        }
    }
}
